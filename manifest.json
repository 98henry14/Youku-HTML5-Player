{
  "manifest_version": 2,
  "name": "Youku HTML5 Player",
  "version": "1.1.1",
  "description": "告别flash与广告\n\n开源仓库：https://github.com/esterTion/Youku-HTML5-Player\n本扩展使用组件：\nflvjs：(github)esterTion/flv.js\nABPlayer-billibili-ver(有修改)",
  "content_scripts": [
    {
      "matches": [
        "*://v.youku.com/v_show/*"
      ],
      "all_frames": true,
      "js": [
        "hookFetch.js",
        "crypto/tripledes.js",
        "crypto/mode-ecb.js",
        "crypto/pad-zeropadding.js",
        "crypto/md5.js",
        "flv.min.js",
        "jquery-2.1.4.min.js",
        "google-style-loading.min.js",
        "CommentCoreLibrary.min.js",
        "biliplus_shield.min.js",
        "ABPlayer.min.js",
        "youku_html5.js"
      ],
      "css": [
        "ABPlayer.css"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "hookFetch_background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "*://*/*"
  ],
  "icons":{
    "48":"icon.png"
  }
}